{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*"
  ],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "esnext",
    "composite": true,
    "baseUrl": ".",
    "types": [
      "electron-vite/node"
    ],
    "paths": {
      "@/*": [
        "./src/renderer/src/*"
      ]
    }
  }
}