    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-commonjs": "^0.10.4"
  }
