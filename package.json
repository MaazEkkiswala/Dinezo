    "lucide-react": "^0.542.0",
    "moment": "^2.30.1",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.9.0",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
